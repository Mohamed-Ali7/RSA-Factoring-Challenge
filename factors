#!/usr/bin/python3

import sys
from math import sqrt


file =  open(sys.argv[1], "r")

number = 0
p = 0

for line in file:
    number = int(line)
    _sqrt = int(sqrt(number)) + 1

    for q in range(2, _sqrt):
        if number % q == 0:
            p = number//q
            print(f"{number}={p}*{q}")
            break
        if number % _sqrt == 0:
            p = number // (_sqrt - q)
            print(f"{number}={_sqrt - 1}*{p}")
            break
        _sqrt -= 1

file.close()
