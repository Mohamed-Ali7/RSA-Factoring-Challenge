#!/usr/bin/python3
import sys
from decimal import Decimal

if len(sys.argv) != 2:
    print('Usage: factors <file>')
    sys.exit(1)
file =  open(sys.argv[1], "r")

for line in file:
    number = int(line)
    n = Decimal(number).sqrt() + 1
    for p in range(2, int(n)):
        if number % p == 0:
            q = number // p
            print(f"{number}={q}*{p}")
            break
        elif number % int(n) == 0:
            q = number // int(n)
            print(f"{number}={q}*{n}")
            break
        n -= 1
file.close()
